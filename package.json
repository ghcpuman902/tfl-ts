{
  "name": "tfl-ts",
  "version": "1.0.2",
  "description": "ðŸš‡ Fully-typed TypeScript client for Transport for London (TfL) API â€¢ Zero dependencies â€¢ Auto-generated types â€¢ Real-time arrivals â€¢ Journey planning â€¢ Universal compatibility",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/utils/",
    "dist/generated/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run generate:types && tsc",
    "build:full": "npm run generate && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "playground": "ts-node playground/index.ts",
    "demo": "ts-node playground/demo.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate": "npm run generate:types && npm run generate:meta",
    "generate:types": "swagger-typescript-api -p https://api.tfl.gov.uk/swagger/docs/v1 -o ./src/generated -n tfl.ts --union-enums --extract-request-params --patch --extract-enums && ts-node script/generateJsDoc.ts",
    "generate:meta": "ts-node script/generateMeta.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run build",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "tfl",
    "transport-for-london",
    "typescript",
    "api",
    "client",
